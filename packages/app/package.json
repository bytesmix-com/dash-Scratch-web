    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "^4.6.1",
    "babel-loader": "8.2.2",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^6.2.1",
    "dotenv": "^10.0.0",
    "eslint": "7.17.0",
    "eslint-config-airbnb": "19.0.1",
    "eslint-config-prettier": "^6.11.0",
