{
  "name": "@scratch-tutoring-web/app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "emp dev",
    "dev:hot": "cp ./.env.dev .env && concurrently \"emp dev -rm --hot\" \"yarn graphql-codegen --watch\"",
    "build": "yarn graphql-codegen && printenv && emp build -t",
    "build:ts": "cp ./.env.prod .env && yarn graphql-codegen && emp build -t",
    "pull-types": "node ./scripts/pull-types.js",
    "serve": "emp serve",
    "build:watch": "emp build -t --watch && yarn serve",
    "type-check": "tsc --noEmit",
    "lint": "eslint ./ --ext ts --ext tsx --color",
    "lint:fix": "yarn lint --fix --color",
    "graphql-codegen": "graphql-codegen -r dotenv/config"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.1",
