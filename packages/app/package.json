    "@dicolabs-kr/web.ui.base.page-shell.app-page-shell": "^0.0.5",
    "@dicolabs-kr/web.ui.base.svg-icon": "^0.0.11",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@fontsource/kanit": "^4.5.0",
    "@react-hookz/web": "^12.0.0",
    "@types/react-table": "^7.7.9",
    "date-fns": "^2.27.0",
    "framer-motion": "^4",
    "graphql": "^15.6.0",
    "graphql-request": "^3.7.0",
    "lodash": "^4.17.21",
    "nuka-carousel": "^4.7.9",
    "pretendard": "^1.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.21.0",
    "react-icons": "^4.3.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-nanny": "^2.11.0",
    "react-player": "^2.9.0",
    "react-query": "^3.34.2",
    "react-rnd": "^10.3.5",
    "react-router-dom": "^6.0.2",
    "react-table": "^7.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/preset-react": "7.14.5",
    "@chakra-ui/cli": "^1.6.0",
